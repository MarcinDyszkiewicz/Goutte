{
    "name": "fabpot/goutte",
    "type": "application",
    "description": "A simple PHP Web Scraper",
    "keywords": ["scraper"],
    "homepage": "https://github.com/FriendsOfPHP/Goutte",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/browser-kit": "^4.3",
        "symfony/css-selector": "~3.4|~4.0",
        "symfony/dom-crawler": "dev-dev-fix-public-nodes",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.3 || ^4"
    },
        "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/marcindyszkiewicz/dom-crawler"
        }
        ],
    "autoload": {
        "psr-4": { "Goutte\\": "Goutte" },
        "exclude-from-classmap": ["Goutte/Tests"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    }
}
